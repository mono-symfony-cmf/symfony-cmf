{
    "name": "symfony-cmf/testing",
    "license": "MIT",
    "description": "Component providing tools for writing tests with Symfony.",
    "authors": [
        {
            "name": "Symfony CMF Community",
            "homepage": "https://github.com/symfony-cmf/Routing/contributors"
        }
    ],
    "require": {
        "php": "^5.6 || ^7.0",
        "doctrine/common": "^2.7",
        "doctrine/data-fixtures": "^1.2",
        "doctrine/doctrine-bundle": "^1.8",
        "doctrine/phpcr-bundle": "^1.3",
        "doctrine/phpcr-odm": "^1.4 || ^2.0",
        "friendsofsymfony/jsrouting-bundle": "^1.1",
        "jackalope/jackalope": "^1.3",
        "jackalope/jackalope-doctrine-dbal": "^1.3",
        "symfony/browser-kit": "^2.8 || ^3.0",
        "symfony/console": "^2.8 || ^3.0",
        "symfony/css-selector": "^2.8 || ^3.0",
        "symfony/debug": "^2.8 || ^3.0",
        "symfony/dependency-injection": "^2.8 || ^3.0",
        "symfony/doctrine-bridge": "^2.8 || ^3.0",
        "symfony/framework-bundle": "^2.8.1 || ^3.0",
        "symfony/http-foundation": "^2.8 || ^3.0",
        "symfony/http-kernel": "^2.8 || ^3.0",
        "symfony/monolog-bundle": "^2.8 || ^3.0",
        "symfony/process": "^2.8 || ^3.0",
        "symfony/security-bundle": "^2.8 || ^3.0",
        "symfony/twig-bundle": "^2.8 || ^3.0"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "Symfony\\Cmf\\Component\\Testing\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Symfony\\Cmf\\Component\\Testing\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev"
        }
    }
}
