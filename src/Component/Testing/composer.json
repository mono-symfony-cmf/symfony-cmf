{
    "name": "symfony-cmf/testing",
    "license": "MIT",
    "description": "Component providing tools for writing tests with Symfony.",
    "authors": [
        {
            "name": "Symfony CMF Community",
            "homepage": "https://github.com/symfony-cmf/Routing/contributors"
        }
    ],
    "require": {
        "php": "^5.6|^7.0",
        "symfony/symfony": "^2.8|^3.0",
        "symfony/phpunit-bridge": "^3.2",
        "symfony/monolog-bundle": "^2.1|^3.0",
        "doctrine/common": "^2.4",
        "doctrine/doctrine-bundle": "^1.0",
        "doctrine/phpcr-odm": "^1.1|^2.0",
        "doctrine/phpcr-bundle": "^1.1",
        "doctrine/data-fixtures": "^1.0",
        "jackalope/jackalope": "^1.1.5",
        "jackalope/jackalope-doctrine-dbal": "^1.1.5",
        "friendsofsymfony/jsrouting-bundle": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "Symfony\\Cmf\\Component\\Testing\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Symfony\\Cmf\\Component\\Testing\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev"
        }
    }
}
