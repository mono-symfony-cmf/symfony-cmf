{
    "name": "symfony-cmf/resource-bundle",
    "description": "Bundle which facilitates document resource location",
    "homepage": "http://cmf.symfony.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Symfony CMF Community",
            "homepage": "https://github.com/symfony-cmf/symfony-cmf/contributors"
        }
    ],
    "require": {
        "php": "^7.1",
        "symfony/framework-bundle": "^2.8 || ^3.3 || ^4.0",
        "symfony/options-resolver": "^2.8 || ^3.3 || ^4.0",
        "symfony-cmf/resource": "^1.0"
    },
    "require-dev": {
        "symfony-cmf/testing": "^2.1@dev",
        "symfony/phpunit-bridge": "^3.3 || ^4.0",
        "doctrine/phpcr-odm": "^1.4|^2.0"
    },
    "suggest": {
        "doctrine/phpcr-odm": "To enable support for the PHPCR ODM documents (^1.3)",
        "doctrine/phpcr-bundle": "To enable support for the PHPCR ODM documents"
    },
    "autoload": {
        "psr-4": {
            "Symfony\\Cmf\\Bundle\\ResourceBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Symfony\\Cmf\\Bundle\\ResourceBundle\\Tests\\": "tests/",
            "Symfony\\Cmf\\Bundle\\ResourceBundle\\Tests\\App\\": "tests/Resources/app"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.1-dev"
        }
    },
    "type": "symfony-bundle"
}
