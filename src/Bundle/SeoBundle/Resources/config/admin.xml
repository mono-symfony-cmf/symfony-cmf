<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>
        <parameter key="cmf_seo.admin_extension.class">Symfony\Cmf\Bundle\SeoBundle\Admin\Extension\SeoContentAdminExtension</parameter>
    </parameters>

    <services>
        <service id="cmf_seo.admin_extension" class="%cmf_seo.admin_extension.class%">
            <argument>%cmf_seo.sonata_admin_extension.form_group%</argument>
            <tag name="sonata.admin.extension" />
        </service>
    </services>
</container>
